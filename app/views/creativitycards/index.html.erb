<div class="bs-docs-section">
  <div class="row">
    <div class="col-md-12">
      <h1>Kreativismen</h1>
      
    </div>
  </div>
</div> 



<table>
  <thead>
    <tr>
      <th>Suggestion</th>
      <th>Idea</th>
      <th>Project</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @creativitycards.each do |creativitycard| %>
      <tr>
        <td><%= creativitycard.suggestion %></td>
        <td><%= creativitycard.idea_id %></td>
        <td><%= creativitycard.project_id %></td>
        <td><%= creativitycard.user_id %></td>
        <td><%= link_to 'Show', creativitycard %></td>
        <td><%= link_to 'Edit', edit_creativitycard_path(creativitycard) %></td>
        <td><%= link_to 'Destroy', creativitycard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Creativitycard', new_creativitycard_path %>
